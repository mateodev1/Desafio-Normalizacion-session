<%- include("layouts/main.ejs") %>

<h1>Centro de mensajes</h1>
<div class="container-chat" >
    <input placeholder="Email" type="email" id="email" required>
    <input placeholder="Nombre" type="email" id="name" required>
    <input placeholder="Apellido" type="email" id="lastName" required>
    <input placeholder="Edad" type="email" id="age" required>
    <input placeholder="Alias" type="email" id="alias" required>
    <input placeholder="Avatar" type="email" id="avatar" required>
    <div id="chat"></div>
    <div>
        <input placeholder="Ingresa un mensaje..." type="text" id="message" required>
        <button type="button" id="btn-chat" class="btn">Enviar</button>
    </div>
</div>
</div>


<script src="/socket.io/socket.io.js"></script>
<script src="./../static/socket.js"></script>